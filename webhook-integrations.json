  "custom": {
    "name": "Custom REST API Integrations",
    "description": "Pre-built integrations for custom applications and APIs",
    "templates": {
      "python-client": {
        "name": "Python SDK Client",
        "language": "python",
        "package": "requests",
        "example": {
          "code": "import requests\n\nclass TARSClient:\n    def __init__(self, token, base_url='http://localhost:18790'):\n        self.token = token\n        self.base_url = base_url\n        self.headers = {'Authorization': f'Bearer {token}'}\n    \n    def add_task(self, task, priority='medium'):\n        return requests.post(\n            f'{self.base_url}/webhooks/v2/task',\n            json={'action': 'add_task', 'task': task, 'priority': priority},\n            headers=self.headers\n        ).json()\n    \n    def notify(self, title, body, priority='P2'):\n        return requests.post(\n            f'{self.base_url}/webhooks/v2/notify',\n            json={'action': 'send_notification', 'title': title, 'body': body, 'priority': priority},\n            headers=self.headers\n        ).json()",
          "usage": "client = TARSClient('your-api-key')\nresult = client.add_task('Research AI frameworks')"
        }
      },

      "node-client": {
        "name": "Node.js SDK Client",
        "language": "javascript",
        "package": "axios",
        "example": {
          "code": "const axios = require('axios');\n\nclass TARSClient {\n  constructor(token, baseUrl = 'http://localhost:18790') {\n    this.token = token;\n    this.client = axios.create({\n      baseURL: baseUrl,\n      headers: { Authorization: `Bearer ${token}` }\n    });\n  }\n  \n  async addTask(task, priority = 'medium') {\n    return this.client.post('/webhooks/v2/task', {\n      action: 'add_task',\n      task,\n      priority\n    });\n  }\n  \n  async notify(title, body, priority = 'P2') {\n    return this.client.post('/webhooks/v2/notify', {\n      action: 'send_notification',\n      title,\n      body,\n      priority\n    });\n  }\n}",
          "usage": "const client = new TARSClient('your-api-key');\nawait client.addTask('Research quantum computing');"
        }
      },

      "curl-examples": {
        "name": "cURL Examples",
        "language": "bash",
        "examples": {
          "add_task": "curl -X POST http://localhost:18790/webhooks/v2/task \\\n  -H 'Authorization: Bearer YOUR_TOKEN' \\\n  -H 'Content-Type: application/json' \\\n  -d '{\n    \"action\": \"add_task\",\n    \"task\": \"Research quantum computing\",\n    \"priority\": \"high\"\n  }'",
          "send_notification": "curl -X POST http://localhost:18790/webhooks/v2/notify \\\n  -H 'Authorization: Bearer YOUR_TOKEN' \\\n  -H 'Content-Type: application/json' \\\n  -d '{\n    \"action\": \"send_notification\",\n    \"title\": \"Test Alert\",\n    \"body\": \"This is a test notification\",\n    \"priority\": \"P2\"\n  }'",
          "start_research": "curl -X POST http://localhost:18790/webhooks/v2/research \\\n  -H 'Authorization: Bearer YOUR_TOKEN' \\\n  -H 'Content-Type: application/json' \\\n  -d '{\n    \"action\": \"start_research\",\n    \"topic\": \"Latest AI developments\",\n    \"depth\": 2,\n    \"format\": \"markdown\"\n  }'"
        }
      },

      "powershell-examples": {
        "name": "PowerShell Examples",
        "language": "powershell",
        "examples": {
          "add_task": "$headers = @{'Authorization' = 'Bearer YOUR_TOKEN'}\n$body = @{\n  action = 'add_task'\n  task = 'Research quantum computing'\n  priority = 'high'\n} | ConvertTo-Json\n\nInvoke-RestMethod -Uri 'http://localhost:18790/webhooks/v2/task' \\\n  -Method POST \\\n  -Headers $headers \\\n  -Body $body \\\n  -ContentType 'application/json'",
          "send_notification": "$headers = @{'Authorization' = 'Bearer YOUR_TOKEN'}\n$body = @{\n  action = 'send_notification'\n  title = 'Test Alert'\n  body = 'This is a test'\n  priority = 'P2'\n} | ConvertTo-Json\n\nInvoke-RestMethod -Uri 'http://localhost:18790/webhooks/v2/notify' \\\n  -Method POST \\\n  -Headers $headers \\\n  -Body $body \\\n  -ContentType 'application/json'"
        }
      }
    }
  },

